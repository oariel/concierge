[
    {
        "fdata": {
            "category": "Employees",
            "steps": [
                {
                    "fn_name": "getinput",
                    "followup": "Which employee are you searching for?",
                    "variable_name": "@Name"
                },
                {
                    "name": "Search for an employee",
                    "fn_name": "spaceiqquery",
                    "query": "query { viewer { company { search(query: \"@Name\", type: employees) { employees(first: 10) { edges { node { name id } } } } } } }",
                    "step_result": {
                        "name": "name",
                        "value": "name"
                    }
                },
                {
                    "name": "Employee information",
                    "fn_name": "spaceiqquery",
                    "query": "query { viewer { company { search(query: \"{0}\", type: employees) { employees(first: 10) { edges { node { name assets{ url(style: \"thumbnail\") } department { name } team { name } seat { external_id code floor { name code } building { name code } } neighborhoods { name floor { id name code } building { name code } } } } } spaces(first: 10) { edges { node { name code externalId capacity assets{ url(style: \"thumbnail\") } department { name } building { name code } floor { name code } } } } } } } }",
                    "step_result": {
                        "name": "external_id",
                        "value": "external_id",
                        "include": true
                    },
                    "followup": "Select from the following employees that match your search"
                },
                {
                    "name": "Show map",
                    "fn_name": "spaceiqquery",
                    "query": "query { viewer { company { mapAsset(zoom: 1.5, filter:{key:SpaceCode,value:\"{0}\"}){ url } } } }"
                }
            ]
        },
        "phrases": [
            "Find employee",
            "Look for an employee",
            "Find someone"
        ]
    },
